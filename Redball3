<script>

function moveup() {
if(redball.top<10) 
{alert('stopit')
}else {redball.top=redball.top -10;}
	
}

	redball.top=redball.top -10; 
}

</script>

<script>
function movedown() {
	redball.top=redball.top +10;
}

</script>

<script>
function moveleft() {
	redball.left=redball.left -10;
}

</script>

<script>
function moveright() {
	redball.left=redball.left +10;
}

</script>

