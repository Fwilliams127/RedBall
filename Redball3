	movedownright();
	movedown();
	movedown();
	movedown();
	movedownleft();
	movedownleft();
	movedownleft();
	moveleft();
	moveleft();
	moveleft();
	moveupleft();
	moveupleft();
	moveupleft();
	moveup();
	moveup();
	moveup();
	moveupright();
	
}

</script>	

